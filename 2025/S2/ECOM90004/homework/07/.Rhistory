library(urca)
library(forecast)
library(tidyverse)
# Convert matrices to long tibble
sel_tbl <- as.data.frame(AICc) |>
tibble::rownames_to_column("p") |>
tidyr::pivot_longer(-p, names_to = "h", values_to = "AICc") |>
mutate(LBp = as.vector(LBp),
p = as.integer(sub("p=", "", p)),
h = as.integer(sub("h=", "", h)))
